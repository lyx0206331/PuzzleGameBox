<resources>

    <!-- Base application theme. -->
    <style name="modulegomoku_AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/modulegomoku_colorPrimary</item>
        <item name="colorPrimaryDark">@color/modulegomoku_colorPrimaryDark</item>
        <item name="colorAccent">@color/modulegomoku_colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="modulegomoku_Theme.AppStartLoad" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="android:windowBackground">@drawable/nav_0</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

</resources>
