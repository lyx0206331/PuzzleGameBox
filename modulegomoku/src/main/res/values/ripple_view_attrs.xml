<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="modulegomoku_RippleView">
        <attr name="modulegomoku_rv_alpha" format="integer" />
        <attr name="modulegomoku_rv_framerate" format="integer" />
        <attr name="modulegomoku_rv_rippleDuration" format="integer" />
        <attr name="modulegomoku_rv_zoomDuration" format="integer" />
        <attr name="modulegomoku_rv_color" format="color|reference" />
        <attr name="modulegomoku_rv_centered" format="boolean" />
        <attr name="modulegomoku_rv_type" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="modulegomoku_rv_ripplePadding" format="dimension" />
        <attr name="modulegomoku_rv_zoom" format="boolean" />
        <attr name="modulegomoku_rv_zoomScale" format="float" />

    </declare-styleable>
</resources>